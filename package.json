{
  "name": "multimedia-api",
  "version": "1.0.0",
  "description": "API for adding multimedia features (video/voice chat) to web apps with Stripe billing",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "echo 'No build step required'",
    "test": "echo 'Tests not implemented yet'",
    "deploy": "bash deploy.sh",
    "docker:build": "docker build -t multimedia-api .",
    "docker:run": "docker run -p 3000:3000 multimedia-api",
    "setup:production": "cp .env.example .env.production"
  },
  "keywords": ["webrtc", "video-chat", "voice-chat", "multimedia", "api", "stripe", "billing", "saas"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.0",
    "stripe": "^14.12.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}